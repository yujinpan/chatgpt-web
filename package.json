{
  "name": "chatgpt-web",
  "version": "0.0.13",
  "author": "yujinpan",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "npm run encrypt && vite build",
    "preview": "vite preview --port 4173",
    "publish:beta": "release-ops tag beta && npm run push",
    "publish:patch": "release-ops tag patch && npm run push",
    "push": "git push && git push --tags && sh deploy.sh",
    "test": "vitest --environment jsdom",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "encrypt": "node scripts/encrypt.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.11.4",
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "vue": "^3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "dotenv": "^16.1.4",
    "eslint": "8.40.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.13.0",
    "javascript-obfuscator": "^4.0.2",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "path-ops": "^1.0.0",
    "prettier": "^2.8.8",
    "release-ops": "^1.0.1",
    "sass": "^1.62.1",
    "typescript": "~4.7.4",
    "vite": "^5.3.0",
    "vite-plugin-ga": "^1.0.2",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
